{
  "expo": {
    "name": "Shanti App",
    "slug": "Deep-Breathing-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/assets/icon1.png",
    "splash": {
      "image": "./App/assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffe57f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/adaptive-icon1.png",
        "backgroundColor": "#ffe57f"
      }
    },
    "web": {
      "favicon": "./App/assets/favicon.png"
    }
  }
}
